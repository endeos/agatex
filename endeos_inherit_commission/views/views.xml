<odoo>
    <data>
        <record id="view_sale_order_line_agent_tree_inherit" model="ir.ui.view">
            <field name="name">sale.order.line.agent.tree.inherit</field>
            <field name="model">sale.order.line.agent</field>
            <field name="inherit_id" ref="sale_commission.view_sale_order_line_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='commission_id']" position="after">
                    <field name="all_commission_id"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
